# Environment variables template
# Copy this file to .env and update with your values

# Application settings
APP_NAME="LangChain Documentation Server"
DEBUG=false
LOG_LEVEL=INFO

# API Configuration
LANGCHAIN_DOCS_BASE=https://python.langchain.com
GITHUB_API_BASE=https://api.github.com/repos/langchain-ai/langchain
REQUEST_TIMEOUT=30

# Server settings
HOST=0.0.0.0
PORT=8000

# Cache settings (if using Redis)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
CACHE_TTL=300

# Rate limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Optional: GitHub token for higher API limits
# GITHUB_TOKEN=your_github_token_here
